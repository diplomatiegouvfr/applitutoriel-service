<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="civilite">


	<resultMap type="fr.gouv.diplomatie.applitutoriel.business.bo.Civilite" id="mapCivilite">
		<result column="ID_CIVILITE" property="id" />
		<result column="CIV_LIBELLE" property="libelle" />
	</resultMap>

    <select id="select" resultMap="mapCivilite">
        SELECT ID_CIVILITE, CIV_LIBELLE
        FROM CIVILITE ;
    </select>

    <select id="selectByLibelle" resultMap="mapCivilite">
        SELECT ID_CIVILITE, CIV_LIBELLE
        FROM CIVILITE
        WHERE CIV_LIBELLE = #{libelle};
    </select>
</mapper>